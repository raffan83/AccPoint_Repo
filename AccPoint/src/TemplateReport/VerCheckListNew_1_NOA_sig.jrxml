				<text><![CDATA[Tipologia di controllo]]></text>
			</staticText>
			<staticText>
				<reportElement x="418" y="84" width="44" height="21" uuid="53b9228b-2b3f-4f8a-9cc4-ea26f4f692ae"/>
				<box topPadding="0" leftPadding="0" bottomPadding="0" rightPadding="0">
					<pen lineWidth="1.0"/>
				</box>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font isBold="true" isItalic="true" pdfFontName="Helvetica-Bold"/>
				</textElement>
				<text><![CDATA[SÌ]]></text>
			</staticText>
			<staticText>
				<reportElement x="462" y="84" width="44" height="21" uuid="6c361a33-aa9c-4bfb-848a-bf203609345c"/>
				<box topPadding="0" leftPadding="0" bottomPadding="0" rightPadding="0">
					<pen lineWidth="1.0"/>
				</box>
