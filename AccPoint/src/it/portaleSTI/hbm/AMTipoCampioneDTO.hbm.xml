<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="it.portaleSTI.DTO.AMTipoCampioneDTO" table="am_tipo_campione">
       
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="denominazione" type="java.lang.String">
            <column name="denominazione" />
        </property>
        
        <property name="visibilitaColonne" type="java.lang.String">
            <column name="visibilita_colonne" />
        </property>
    </class>
</hibernate-mapping>
