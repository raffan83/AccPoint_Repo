<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="it.portaleSTI.DTO.DevAllegatiSoftwareDTO" table="dev_allegati_software">
	        <id name="id" type="int">
            <column name="Id" />
            <generator class="native" />
        </id>	
       <property name="nome_file" type="java.lang.String">
            <column name="nome_file" />
        </property>

        
	<property name="id_software" type="int">
            <column name="id_software" />
        </property>
     <property name="disabilitato" type="int">
            <column name="disabilitato" />
        </property>
       
	</class>
</hibernate-mapping>