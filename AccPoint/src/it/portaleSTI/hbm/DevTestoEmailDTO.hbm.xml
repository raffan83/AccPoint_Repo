<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="it.portaleSTI.DTO.DevTestoEmailDTO" table="dev_testo_email">
	        <id name="id" type="int">
            <column name="Id" />
            <generator class="native" />
        </id>	
       <property name="descrizione" type="java.lang.String">
            <column name="descrizione" />
        </property>
        
   <property name="referenti" type="java.lang.String">
            <column name="referenti" />
        </property>
        
         <property name="sollecito" type="java.lang.String">
            <column name="sollecito" />
        </property>
	
	</class>
</hibernate-mapping>