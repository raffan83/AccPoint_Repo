<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="it.portaleSTI.DTO.DpiAllegatiDTO" table="dpi_allegati">
	        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>	
       <property name="nome_file" type="java.lang.String">
            <column name="nome_file" />
        </property>
        
        	<property name="id_dpi" type="int">
            <column name="id_dpi" />
        </property>

        
	<property name="id_manuale" type="int">
            <column name="id_dpi_manuale" />
        </property>
     
       <property name="disabilitato" type="int">
            <column name="disabilitato" />
        </property>
        
        <property name="data_caricamento" type="java.sql.Date">
            <column name="data_caricamento" />
        </property>
	</class>
</hibernate-mapping>