<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="it.portaleSTI.DTO.GPDTO" table="gp_greenpass">
       
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
 		
   
        <property name="cognome" type="java.lang.String">
            <column name="cognome" />
        </property>
        
         <property name="nome" type="java.lang.String">
            <column name="nome" />
        </property>
        
         <property name="dataNascita" type="java.util.Date">
            <column name="dataNascita" />
        </property>
        
          <property name="esito" type="java.lang.String">
            <column name="esito" />
        </property>
        
            <property name="dataVerifica" type="java.util.Date">
            <column name="dataVerifica" />
        </property>
        <property name="visibile" type="java.lang.Integer">
            <column name="visibile" />
        </property>

    </class>
</hibernate-mapping>
