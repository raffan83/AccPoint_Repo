<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="it.portaleSTI.DTO.OffOffertaDTO" table="off_offerta">
	
	        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
           <property name="id_cliente" type="int">
            <column name="id_cliente" />
        </property>
       <property name="id_sede" type="int">
            <column name="id_sede" />
        </property>
       <property name="nome_cliente" type="java.lang.String">
            <column name="nome_cliente" />
        </property>
       <property name="nome_sede" type="java.lang.String">
            <column name="nome_sede" />
        </property>
          <property name="importo" type="java.lang.Double">
            <column name="importo" />
        </property>
          <property name="note" type="java.lang.String">
            <column name="note" />
        </property>
         <property name="n_offerta" type="java.lang.String">
            <column name="n_offerta" />
        </property>
            
        <property name="data_offerta" type="java.util.Date">
            <column name="data_offerta" />
        </property>
        
         <property name="stato" type="int">
            <column name="stato" />
        </property>
         <many-to-one name="utente" class="it.portaleSTI.DTO.UtenteDTO" fetch="join" lazy="false">
            <column name="id_utente" />
        </many-to-one> 
              
	</class>
</hibernate-mapping>