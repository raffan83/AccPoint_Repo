<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="it.portaleSTI.DTO.RilInterventoDTO" table="ril_intervento">
	        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>	
       <property name="id_cliente" type="int">
            <column name="id_cliente" />
        </property>
           <property name="id_sede" type="int">
            <column name="id_sede" />
        </property>
        <property name="stato_intervento" type="int">
            <column name="stato_intervento" />
        </property>
        <property name="nome_cliente" type="java.lang.String">
            <column name="nome_cliente" />
        </property>
           <property name="nome_sede" type="java.lang.String">
            <column name="nome_sede" />
        </property>
            <property name="data_chiusura" type="java.sql.Date">
            <column name="data_chiusura_intervento" />
        </property>
            <property name="data_apertura" type="java.sql.Date">
            <column name="data_apertura_intervento" />
        </property>
        <property name="commessa" type="java.lang.String">
            <column name="commessa" />
        </property>
              <property name="id_pacco" type="java.lang.Integer">
            <column name="id_pacco" />
        </property>
	</class>
</hibernate-mapping>