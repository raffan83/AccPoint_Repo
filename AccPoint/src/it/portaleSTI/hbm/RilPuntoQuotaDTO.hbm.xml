<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">



<hibernate-mapping>
	<class name="it.portaleSTI.DTO.RilPuntoQuotaDTO" table="ril_punto_quota">
	        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>	
       
   		<property name="id_quota" type="int">
            <column name="id_quota" />
        </property>
 		
 		<property name="valore_punto" type="java.lang.String">
            <column name="valore_punto" />
        </property>
       
	</class>
</hibernate-mapping>