<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
    <class name="it.portaleSTI.DTO.VerDecentramentoDTO" table="ver_decentramento">
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="idMisura" type="int">
            <column name="id_misura" />
        </property>

        <property name="tipoRicettore" type="int">
            <column name="tipo_ricettore" />
        </property>
        <property name="puntiAppoggio" type="int">
            <column name="punti_appoggio" />
        </property>
        <property name="campo" type="int">
            <column name="campo" />
        </property>
        <property name="posizione" type="int">
            <column name="posizione" />
        </property>
        <property name="massa" type="java.math.BigDecimal">
            <column name="massa" />
        </property>
       <property name="indicazione" type="java.math.BigDecimal">
            <column name="indicazione" />
        </property>
          <property name="caricoAgg" type="java.math.BigDecimal">
            <column name="carico_agg" />
        </property>
        <property name="errore" type="java.math.BigDecimal">
            <column name="errore" />
        </property>
         <property name="erroreCor" type="java.math.BigDecimal">
            <column name="errore_cor" />
        </property> 
        <property name="mpe" type="java.math.BigDecimal">
            <column name="mpe" />
        </property>
         <property name="esito" type="java.lang.String">
            <column name="esito" />
        </property>
         <property name="carico" type="java.math.BigDecimal">
            <column name="carico" />
        </property>
          <property name="speciale" type="java.lang.String">
            <column name="speciale" />
        </property>
        
    </class>
</hibernate-mapping>