<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping >
    <class name="it.portaleSTI.DTO.VerLinearitaDTO" table="ver_linearita">
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
         <property name="idMisura" type="int">
            <column name="id_misura" />
        </property>
 
        <property name="tipoAzzeramento" type="int">
            <column name="tipo_azzeramento" />
        </property>
        <property name="campo" type="int">
            <column name="campo" />
        </property>
        <property name="riferimento" type="int">
            <column name="riferimento" />
        </property>
        <property name="massa" type="java.math.BigDecimal">
            <column name="massa" />
        </property>
       <property name="indicazioneSalita" type="java.math.BigDecimal">
            <column name="indicazione_salita" />
        </property>
        <property name="indicazioneDiscesa" type="java.math.BigDecimal">
            <column name="indicazione_discesa" />
        </property>
          <property name="caricoAggSalita" type="java.math.BigDecimal">
            <column name="carico_agg_salita" />
        </property>
          <property name="caricoAggDiscesa" type="java.math.BigDecimal">
            <column name="carico_agg_discesa" />
        </property>
        <property name="erroreSalita" type="java.math.BigDecimal">
            <column name="errore_salita" />
        </property>
        <property name="erroreDiscesa" type="java.math.BigDecimal">
            <column name="errore_discesa" />
        </property>
         <property name="erroreCorSalita" type="java.math.BigDecimal">
            <column name="errore_cor_salita" />
        </property> 
        <property name="erroreCorDiscesa" type="java.math.BigDecimal">
            <column name="errore_cor_discesa" />
        </property> 
        <property name="mpe" type="java.math.BigDecimal">
            <column name="mpe" />
        </property>
        <property name="divisione" type="java.math.BigDecimal">
            <column name="divisione" />
        </property>
         <property name="esito" type="java.lang.String">
            <column name="esito" />
        </property>
         <property name="posizione_salita" type="java.lang.String">
            <column name="posizione_salita" />
        </property>
         <property name="posizione_discesa" type="java.lang.String">
            <column name="posizione_discesa" />
        </property>
        
    </class>
</hibernate-mapping>